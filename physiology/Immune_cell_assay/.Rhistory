Pcli_model_metrics <- augment(Pcli_model)
plot(Pcli_model)
# assess normality of residuals using Shapiro-Wilk test
shapiro_test(Pcli_model_metrics$.resid) #not significant
# assess homogeneity of variances using Levene's Test
